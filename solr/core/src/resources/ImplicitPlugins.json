{
  "requestHandler": {
    "/update": {
      "useParams":"_UPDATE",
      "class": "org.apache.solr.handler.UpdateRequestHandler"
    },
    "/update/json": {
      "useParams":"_UPDATE_JSON",
      "class": "org.apache.solr.handler.UpdateRequestHandler",
      "invariants": {
        "update.contentType": "application/json"
      }
    },
    "/update/csv": {
      "useParams":"_UPDATE_CSV",
      "class": "org.apache.solr.handler.UpdateRequestHandler",
      "invariants": {
        "update.contentType": "application/csv"
      }
    },
    "/update/json/docs": {
      "useParams":"_UPDATE_JSON_DOCS",
      "class": "org.apache.solr.handler.UpdateRequestHandler",
      "invariants": {
        "update.contentType": "application/json",
        "json.command": "false"
      }
    },
    "update":{
      "class":"org.apache.solr.handler.UpdateRequestHandlerApi",
      "useParams": "_UPDATE_JSON_DOCS"
    },
    "/config": {
      "useParams":"_CONFIG",
      "class": "org.apache.solr.handler.SolrConfigHandler"
    },
    "/schema": {
      "class": "org.apache.solr.handler.SchemaHandler",
      "useParams":"_SCHEMA"

    },
    "/replication": {
      "class": "org.apache.solr.handler.ReplicationHandler",
      "useParams":"_REPLICATION"
    },
    "/get": {
      "class": "org.apache.solr.handler.RealTimeGetHandler",
      "useParams":"_GET",
      "defaults": {
        "omitHeader": true,
        "wt": "json",
        "indent": true
      }
    },
    "/admin/ping": {
      "class": "org.apache.solr.handler.PingRequestHandler",
      "useParams":"_ADMIN_PING",
      "invariants": {
        "echoParams": "all",
        "q": "{!lucene}*:*"
      }
    },
    "/admin/segments": {
      "class": "org.apache.solr.handler.admin.SegmentsInfoRequestHandler",
      "useParams":"_ADMIN_SEGMENTS"
    },
    "/admin/luke": {
      "class": "org.apache.solr.handler.admin.LukeRequestHandler",
      "useParams":"_ADMIN_LUKE"
    },
    "/admin/system": {
      "class": "org.apache.solr.handler.admin.SystemInfoHandler",
      "useParams":"_ADMIN_SYSTEM"
    },
    "/admin/mbeans": {
      "class": "org.apache.solr.handler.admin.SolrInfoMBeanHandler",
      "useParams":"_ADMIN_MBEANS"
    },
    "/admin/plugins": {
      "class": "org.apache.solr.handler.admin.PluginInfoHandler"
    },
    "/admin/threads": {
      "class": "org.apache.solr.handler.admin.ThreadDumpHandler",
      "useParams":"_ADMIN_THREADS"
    },
    "/admin/properties": {
      "class": "org.apache.solr.handler.admin.PropertiesRequestHandler",
      "useParams":"_ADMIN_PROPERTIES"
    },
    "/admin/logging": {
      "class": "org.apache.solr.handler.admin.LoggingHandler",
      "useParams":"_ADMIN_LOGGING"
    },
    "/admin/health": {
      "class": "org.apache.solr.handler.admin.HealthCheckHandler",
      "useParams":"_ADMIN_HEALTH"
    },
    "/admin/file": {
      "class": "org.apache.solr.handler.admin.ShowFileRequestHandler",
      "useParams":"_ADMIN_FILE"
    },
    "/export": {
      "class": "org.apache.solr.handler.ExportHandler",
      "useParams":"_EXPORT",
      "components": [
        "query"
      ],
      "defaults": {
        "wt": "json"
      },
      "invariants": {
        "rq": "{!xport}",
        "distrib": false
      }
    },
    "/graph": {
      "class": "org.apache.solr.handler.GraphHandler",
      "useParams":"_ADMIN_GRAPH",
      "invariants": {
        "wt": "graphml",
        "distrib": false
      }
    },
    "/stream": {
      "class": "org.apache.solr.handler.StreamHandler",
      "useParams":"_STREAM",
      "defaults": {
        "wt": "json"
      },
      "invariants": {
        "distrib": false
      }
    },
    "/sql": {
      "class": "org.apache.solr.handler.SQLHandler",
      "useParams":"_SQL",
      "defaults": {
        "wt": "json"
      },
      "invariants": {
        "distrib": false
      }
    },
    "/terms": {
      "class": "org.apache.solr.handler.component.SearchHandler",
      "useParams":"_TERMS",
      "components": [
        "terms"
      ]
    },
    "/analysis/document": {
      "class": "org.apache.solr.handler.DocumentAnalysisRequestHandler",
      "startup": "lazy",
      "useParams": "_ANALYSIS_DOCUMENT"
    },
    "/analysis/field": {
      "class": "org.apache.solr.handler.FieldAnalysisRequestHandler",
      "startup": "lazy",
      "useParams": "_ANALYSIS_FIELD"
    },
    "/debug/dump": {
      "class": "org.apache.solr.handler.DumpRequestHandler",
      "useParams": "_DEBUG_DUMP",
      "defaults": {
        "echoParams": "explicit",
        "echoHandler": true
      }
    }
  }
}
